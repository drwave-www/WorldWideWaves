"warn_rules_security_title" = "Iwu & Nche";
"warn_rules_security_text" = "Nnọọ na WorldWideWaves! Anyi ji ọṅụ nabata gị isonye na ihe omume ebili zuru ụwa. Iji mee ka onye obula nwee ahụmịhe dị nche na nke na-atọ ụtọ, biko soro ntuziaka ndị a:";
"warn_general_title" = "Iwu Izugbe:";
"warn_general_item_1" = "Nkwanye ùgwù maka Onye Obula: Kekọrịtara ndị sonyere niile ùgwù, n’agbanyeghị ebe ha si. Biko debe nkwanye ùgwù; omume ịkpa ókè ma ọ bụ ịkpasu iwe adịghị akwado.";
"warn_general_item_2" = "Kwanyere Ebe Ọha ùgwù: Hapụ ụzọ gafere ka mma, soro iwu na akara obodo, ma zere igbochi okporo ụzọ ma ọ bụ ụzọ mberede.";
"warn_general_item_3" = "Nọ n’ime Mpaghara e Kpebiri: Maka nche gị, nọọ n’ime mpaghara e debere maka ihe omume ma ọ bụrụ na e nwere.";
"warn_general_item_4" = "Jiri ezi uche mee: Ọ bụrụ na ị họrọ ịṅụ, mee ya n’otu n’otu ma soro iwu obodo.";
"warn_general_item_5" = "Belata Mmetụta Gburugburu: Tụfuo ahihia na ihe mkpofu n’ebe e debere ma zere ịkwụpụ mkpofu ka ebe ahụ dị ọcha.";
"warn_general_item_6" = "Kọọrọ Nsogbu: Ọ bụrụ na ị hụ ihe na-enye obi abụọ ma ọ bụ mmadụ nọ n’ọnwụnwa, gwa ndị ọchịchị obodo ozugbo.";
"warn_safety_title" = "Ndụmọdụ Nche:";
"warn_safety_item_1" = "Yi Ihe Kwesịrị Ekwesị: Yi uwe na akpụkpọ ụkwụ dị mma kwekọrọ na ihe omume na ihu igwe.";
"warn_safety_item_2" = "Nọgide Na-anwụ Ọṅụṅụ: Were mmiri ma na-aṅụ mmiri mgbe niile, karịsịa n’ihe omume n’ebe okpomọkụ dị elu.";
"warn_safety_item_3" = "Lekọta Ihe Onwe Gị: Debe ihe onwunwe gị n’aka, zere iweta ihe bara nnukwu uru ị naghị achọ.";
"warn_safety_item_4" = "Jiri Enyi Jee: Nọnyere ndị enyi ma ọ bụ mepụta ndị ọhụrụ, lekọta onwe unu ọnụ.";
"warn_safety_item_5" = "Kwadebere maka Mberede: Chekwaa nọmba mberede obodo n’ekwentị gị ma kwurịta ebe izute ndị enyi ma ọ bụrụ na unu kewapụ.";
"warn_emergency_title" = "Usoro Mberede:";
"warn_emergency_item_1" = "Jide n’aka: Chọpụta ụzọ mgbapụ kacha nso na ebe mepere emepe (dịka ogige ntụrụndụ, ọgba aghara) gbara gị gburugburu, soro ntuziaka ndị ọchịchị obodo ma ọ bụrụ na mberede emee.";
"warn_emergency_item_2" = "Nkọọtụ Mberede: Chekwaa nọmba mberede obodo gị na nọmba onye gị na ya na-emekọ ihe mberede n’ekwentị tupu isonye.";
"warn_emergency_item_3" = "Ma ọ bụrụ na Mberede: Kpọọ nọmba mberede obodo, gbapụta nwayọ n’ebe mepere emepe dị nche, nyere ndị ọzọ ma ọ bụrụ na i nwere ike, ma hapụ ụzọ gafere ka mma.";
"warn_legal_title" = "Iwu:";
"warn_legal_item_1" = "Mgbaghara Ibu Ọrụ: Iso na ihe omume WorldWideWaves bụ n’ibu ọrụ gị. Site n’isonye, ị na-ekweta ịhapụ nkwupụta mgbaàmà megide ndị kere ngwa metụtara mmerụ nwere ike ime n’oge ihe omume.";
"warn_legal_item_2" = "Ozi Ebe Ọha: Nke a bụ ngagharị ọha ebe foto/filim site n’aka ndị sonyere ma ọ bụ mgbasa ozi nwere ike ime, dabere na iwu obodo. Anyị anaghị ekwu ikike n’onyonyo gị ma anyị apụghị ijidekwa ndekọ nke ndị ọzọ. Ọ bụrụ na ịchọghị ịpụta, gbalịa zere igwefoto mgbe o kwere mee.";
"faq" = "FAQ";
"faq_access" = "Lelee FAQ";
"faq_question_1" = "Gịnị kpatara na enweghị ihe omume n’obodo m ma ọ bụ mba m?";
"faq_answer_1" = "Anyị họrọwo ọtụtụ iri n’ime obodo kacha juo ụwa mmadụ iji mepụta ebili zuru ụwa nke nwere isi. Ihe mgbochi teknụzụ na nke ọrụ ugbu a na-egbochi anyị ịkwado ebe enweghị oke. Anyị chọpụtara nnọchite anya sara mbara n’okirikiri kọntinent, omenala, na asụsụ—ihe nke na-apụtaghị nke ọma mgbe niile. Anyi nwere olileanya itinye obodo ọzọ n’oge na-abịa.";
"faq_question_2" = "Gịnị bụ ọkọlọtọ kọntinent ndị a?";
"faq_answer_2" = "Anyị kere ọkọlọtọ kọntinent ọhụrụ, nke echiche, iji nyere gị aka ịmata obodo n’ime ndepụta ngwa ngwa ma gosipụta mmụọ ịdị n’otu WorldWideWaves na-akwado. Ọkọlọtọ ndị a abụghị nke ọrụ gọọmenti ma ha dị naanị n’ime ngwa a; a na-emeghị ha iji kpasu iwe ma ọ bụ dochie akara mba ma ọ bụ omenala ọ bụla. E nweghị ọkọlọtọ ọrụ gọọmenti maka kọntinent jiogirafi, ya mere anyị chepụtara nke anyị maka doro anya na ntụrụndụ. Ọ bụrụ na ị masịrị ha, nwere onwe gị iji ha.";
"faq_question_3" = "Ònye nọ n’azụ WorldWideWaves?";
"faq_answer_3" = "WorldWideWaves bụ mgbalị onwe. Enweghị ụlọ ọrụ, òtù ma ọ bụ gọọmenti nọ n’azụ ya. Ndị kere ya chọrọ inye ụzọ bara uru maka ndị na-ajụ otú ha ga-esi nyere aka ime ka ụwa na ọnọdụ ndụ ka mma—mgbalị ịkekọrịta ọtụtụ atụmatụ na-akwado mmepe na ọchịchị ndị mmadụ n’okpuru ọkọlọtọ udo na-enweghị ndọrọ ndọrọ ọchịchị.";
"faq_question_4" = "Nke a bụ mgbalị azụmahịa?";
"faq_answer_4" = "Mba. Ngwa ahụ bụ n’efu kpamkpam. N’agbanyeghị na imewe na mmepe ya were oge na ego nke ndị kere ya, ha enwetaghị uru ego onwe ha site na ya.";
"faq_question_5" = "Enwere m ike iso nyere aka?";
"faq_answer_5" = "Ee, a na-anabata ndị na-enyere aka n'efu mgbe niile. Koodu ngwa a bụ isi mmalite mepere emepe na GitHub, ma ị nwere ike ịkpọtụrụ ndị okike na worldwidewaves@proton.me (azịza a gaghị ekwe nkwa). Ụzọ kacha mma isi nyere aka bụ ịgbasa ozi: kesaa foto na vidio sitere n'omume site n'iji hashtags e tụrụ aro, kpọtụrụ ndị nwere mmetụta n'ịntanetị n'obodo gị ka ha gbasaa ozi, kparịta ya n'obodo gị, ma karịa ihe niile, sonyere ebili mmadụ!";
"faq_question_6" = "Kedu ka m ga-esi mee nnwale ebili?";
"faq_answer_6" = "Pịa bọtịnụ dị n'okpuru iji gbanye ọnọdụ nnwale. Na ihuenyo ihe omume, jiri bọtịnụ oge dị nso na bọtịnụ \"Sonyere ebili\" iji malite nnwale site n'iji njirimara ihe omume a (ụbọchị, oge, ebe). Iji kwụsị nnwale, jiri otu bọtịnụ ahụ. N'ikpeazụ, kwụsị ọnọdụ nnwale site n'iji ikọn nkwụsị dị na ogwe na-anọgide n'elu ihuenyo mgbe i mechara.";
