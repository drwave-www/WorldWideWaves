"infos_core_1" = "Ị ma nke a: ọ bụrụ na a kọọrọ akụkọ ụwa n’akwụkwọ nwere peeji 1,000, ụmụ mmadụ ga-apụta naanị n’ahịrịokwu ikpeazụ nke peeji ikpeazụ.";
"infos_core_2" = "Ma akụkọ mkpirikpi a nwere ike ịkwụsị n’oge karịa ka a tụrụ anya. Anyị nọ naanị anyị n’mbara igwe sara mbara ma kpọrọ nkụ. Anyị na-ebibi ihe niile. Anyị bụ ndị onwe-anya. Anyị bụ ndị obi ọjọọ. Anyị bụ ndị anya ukwu. Anyị bụ ndị na-adịghị ahụ anya. Ma anyị na-achọ ịchịkwa. Ma anyị nwekwara ike inwe obi ebere, anyị na-emepụta ihe ịtụnanya, anyị siri ike, anyị nwekwara ike ịbụ ndị na-enyere n’efu.";
"infos_core_3" = "Ma oge anyị dị ntakịrị. Anyi emeela nnukwu ọganihu.. Anyị nwere ike wuo ụzọ ọzọ: obodo nke ikike, nke ịdị n’otu, nke ọchịchị ndị mmadụ. Ọ dị n’aka anyị!";
"infos_core_4" = "Jikọọ na mmegharị ụwa niile, mmịfe udo. Ka anyị mepụta ebili mmadụ, akaebe nke mmadụ-ọrụ anyị n’ezie, oku siri ike nye ndị ndu anyị. Ka anyị zukọ nke ukwu n’obodo na mba, n’ụwa niile. Ka anyị họrọ udo. Ka anyị họrọ ọṅụ. Ka anyị họrọ okike. Ka anyị họrọ ANYỊ.";
"infos_core_5" = "Gịnị ma ọ bụrụ na ebili otu naanị ya zuru iji kpoo anyị ọnụ?";
"infos_core_6" = "WorldWideWaves bụ ngwa mkpanaka na-adị obere oge nwere ọhụụ: ịhazikarị usoro ebili mmadụ na-agafe nnukwu obodo, gafee mba. A ga-eme ya n’ime otu afọ, na-achọ ịgafe ókè anụ ahụ na omenala, mee ka e nwee ịdị n’otu, obodo, na ahụmịhe mmadụ kesara ọnụ.";
"infos_core_7" = "Na isi ya, WorldWideWaves abụghị naanị ngwa; ọ bụ mmegharị ụwa niile e chepụtara iji kpoo ndị mmadụ ọnụ n’ịgụnye ịdị n’otu na ọṅụ, na ụkpụrụ anyị kesara.";
"infos_core_8" = "Site n’iji nkà na ụzụ mee ihe, WorldWideWaves ga-enye ndị sonyere si n’ụwa niile ohere isonye n’ebili ndị a n’obodo ha, mepụta ihe ngosi anya na mmetụta obi dị ike.";
"infos_core_9" = "N’otu mkpụrụ okwu, WorldWideWaves bụ mgbalị nwere uche ukwuu iji jiri ike nkà na ụzụ rụọ ọrụ ọma n’ezie—ịjikọta mmadụ n’ememme ịdị n’otu, ọṅụ, na ime ihe ọnụ. Site n’ebili ọ bụla, ndị sonyere agaghị enwe nanị onyinye nye nnukwu ihe ngosi, ha ga-enwekwa ma kesaa n’ụwa mmetụta miri emi nke ịbụ otu.";
