"warn_rules_security_title" = "Imithetho Nokuphepha";
"warn_rules_security_text" = "Siyakwamukela ku-WorldWideWaves! Siyajabula ukukwamukela emicimbini yamagagasi omhlaba. Ukuze wonke umuntu abe nolwazi oluphephile nolumnandi, sicela ulandele le miyalelo:";
"warn_general_title" = "Imithetho Ejwayelekile :";
"warn_general_item_1" = "Inhlonipho Kubo Bonke: Phatha bonke ababambiqhaza ngenhlonipho, kungakhathaliseki imvelaphi yabo. Sicela ugcine inhlonipho kubo bonke ababekhona; ukuziphatha ngokucwasa noma ukuhlukumeza akwamukelekile.";
"warn_general_item_2" = "Hlonipha Izikhala Zomphakathi: Gcina izindlela zivulekile, landela imithetho nemibhalo yasendaweni, futhi ugweme ukuvimba ithrafikhi noma ukufinyelela kwezimo eziphuthumayo.";
"warn_general_item_3" = "Hlala Ezindaweni Ezimisiwe: Ngokuphepha kwakho, hlala ngaphakathi ezindaweni ezimakwe zomcimbi uma zikhona.";
"warn_general_item_4" = "Ziphendulele: Uma ukhetha ukuphuza, yenza lokho ngokulinganisela nangokuhambisana nemithetho yasendaweni.";
"warn_general_item_5" = "Nciphisa Umthelela Emvelweni: Lahla yonke imfucumfucu ezitsheni ezibekelwe lokho futhi ugweme ukulahla phansi ukuze izindawo zihlanzeke.";
"warn_general_item_6" = "Bika Okukhathazayo: Uma ubona okuthusayo noma othile osenkingeni, shesha ubikele iziphathimandla zendawo.";
"warn_safety_title" = "Amathiphu Okuphepha:";
"warn_safety_item_1" = "Gqoka Kahle: Gqoka izingubo nezicathulo ezikahle ezifanele imisebenzi yomcimbi nezimo zesimo sezulu.";
"warn_safety_item_2" = "Hlala Uphethwe Amanzi: Letha amanzi futhi uhlale uphuzile, ikakhulukazi emicimbini ezindaweni ezifudumele.";
"warn_safety_item_3" = "Qaphela Izimpahla Zakho: Gcina izimpahla zakho ziphephile futhi ugweme ukuletha izinto eziyigugu ongazidingi.";
"warn_safety_item_4" = "Sebenzisa Uhlelo Lobungani: Hlala nabangani noma wenze abasha, nibhekelane ukuphepha.";
"warn_safety_item_5" = "Lungela Izimo Eziphuthumayo: Londoloza izinombolo zezimo eziphuthumayo zendawo kucingo futhi vumelanani indawo yokuhlangana nabangani uma nehlukanisana.";
"warn_emergency_title" = "Izindlela Zokuphuthumayo:";
"warn_emergency_item_1" = "Hlala Unokuqaphela: Khomba izindlela eziseduze eziphephile zokuphuma nezikhala ezivulekile (isb., izikwele, amapaki) ezikuzungezile, ulandele imiyalo yeziphathimandla zasendaweni uma kuvela isimo esiphuthumayo.";
"warn_emergency_item_2" = "Oxhumana nabo Ezimo Eziphuthumayo: Londoloza inombolo yezimo eziphuthumayo yendawo okuyo kanye noxhumana naye womuntu siqu kucingo ngaphambi kokuba ujoyine.";
"warn_emergency_item_3" = "Uma Kuvela Isimo Esiphuthumayo: Shayela inombolo yezimo eziphuthumayo yendawo, phuma ngokuthula uye endaweni evulekile ephephile, usize abanye uma ukwazi, futhi ugcine izindlela zokufinyelela zivulekile.";
"warn_legal_title" = "Okusemthethweni:";
"warn_legal_item_1" = "Isimemezelo Sokungabophi: Ukubamba iqhaza emicimbini ye-WorldWideWaves kwenziwa ngokuzithandela nangengozi yakho. Ngokubamba iqhaza, uyavuma ukungafaki zimangalo kubadali bohlelo mayelana nokulimala okungenzeka ngesikhathi somcimbi.";
"warn_legal_item_2" = "Isaziso Sesikhala Somphakathi: Lona umhlangano womphakathi lapho ukufotwa/ukuthathwa kwamavidiyo yizivakashi noma yabezindaba kungenzeka, ngokuhambisana nemithetho yasendaweni. Asibangi amalungelo esithombe sakho futhi asikwazi ukulawula ukurekhoda kwabantu besithathu. Uma ukhetha ukungaveli, zama ukugwema amakhamera lapho kungenzeka.";
"faq" = "FAQ";
"faq_access" = "Finyelela i-FAQ";
"faq_question_1" = "Kungani kungekho mcimbi edolobheni lami noma ezweni lami?";
"faq_answer_1" = "Sikhethe amadolobha ambalwa amaningi anabantu abaningi emhlabeni ukuze sakhe igagasi lomhlaba elinencazelo. Izithiyo zobuchwepheshe nezokusebenza okwamanje zisivimba ekusekeleni izindawo ezingenamkhawulo. Sizame ukumela kabanzi amazwekazi, amasiko, nezilimi — okungenakuba kuphelele. Sithemba ukwenezela amanye amadolobha ngokuhamba kwesikhathi.";
"faq_question_2" = "Iyini lezi zifulegi zamazwekazi?";
"faq_answer_2" = "Sakhe izifulegi zamazwekazi zokuqala, eziyimibono ukusiza ukukhombe masinya amadolobha ohlwini nokukhombisa umoya wobunye okhuthazwa i-WorldWideWaves. Lezi zifulegi aziwona asemthethweni futhi zikhona kuphela kulesi sicelo; azihloselwe ukwehlisa noma ukushintsha izimpawu zikaZwelonke noma zamasiko. Njengoba kungekho zifulegi ezisemthethweni zamazwekazi ezindaweni, siklame ezethu ukuze kube sobala futhi kujabulise. Uma uzithanda, zizwe ukhululekile ukuzisebenzisa.";
"faq_question_3" = "Ubani ongemuva kwe-WorldWideWaves?";
"faq_answer_3" = "I-WorldWideWaves umkhankaso wangasese ngokuphelele. Akukho nkampani, inhlangano, noma uhulumeni ongemuva kwawo. Abadali bawo bafuna ukunikeza indlela enokwakha kulabo abazibuza ukuthi bangasiza kanjani ukuthuthukisa umhlaba nezimo zokuphila — umzamo wokulethela ndawonye yonke imizamo ekhona ephambili neyentando yeningi ngaphansi kwephiko elinokuthula, elingezepolitiki.";
"faq_question_4" = "Ingabe lo umzamo wezohwebo?";
"faq_answer_4" = "Cha. Uhlelo lokusebenza lukhululekile ngokuphelele. Nakuba ukwakhiwa nokuthuthukiswa kwalo kudinge isikhathi nemali kubadali, abatholi nzuzo eqondile yezezimali kubo.";
"faq_question_5" = "Ngingafaka isandla kulo mzamo?";
"faq_answer_5" = "Yebo, abazithandela bayamukeleka njalo. Ikhodi yohlelo ivulekile (open-source) ku-GitHub, futhi ungathinta abadali ku worldwidewaves@proton.me (izimpendulo aziqinisekisiwe). Indlela engcono kakhulu yokusiza ukusabalalisa umlayezo: yabelana ngezithombe namavidiyo emicimbini usebenzisa ama-hashtag anconyiwe, xhumana nabathonya bendawo ukuze nikhulise umlayezo, khuluma ngakho nomphakathi wakho, kanti okubaluleke kakhulu, joyina igagasi!";
"faq_question_6" = "Ngingalingisa igagasi kanjani?";
"faq_answer_6" = "Thepha inkinobho engezansi ukuze uvule imodi yokulingisa. Ezikrinini zomcimbi sebenzisa inkinobho yesibali sikhathi eduze nenkinobho ethi \"Join the wave\" ukuze uqale ukulingisa usebenzisa izilungiselelo zalo mcimbi (usuku, isikhathi, indawo). Ukumisa ukulingisa sebenzisa inkinobho efanayo. Ekugcineni, misa imodi yokulingisa usebenzisa isithonjana sokumisa esisingqimbeni esihlala sibonakala lapho usuqedile.";
