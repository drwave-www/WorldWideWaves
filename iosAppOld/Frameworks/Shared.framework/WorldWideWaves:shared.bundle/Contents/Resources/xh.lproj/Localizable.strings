"warn_rules_security_title" = "Imigaqo & Ukhuseleko";
"warn_rules_security_text" = "Wamkelekile kwi-WorldWideWaves! Siyavuya ukukubona ujoyina iziganeko zegagasi zehlabathi. Ukuqinisekisa ukhuseleko nokonwaba kwawo wonke umntu, nceda ulandele le miyalelo:";
"warn_general_title" = "Imigaqo Jikelele :";
"warn_general_item_1" = "Inhlonipho kuWonke: Phatha bonke abathathi-nxaxheba ngentlonipho, kungakhathaliseki imvelaphi yabo. Nceda ugcine inhlonipho kubo bonke ababekhoyo; ukuziphatha okubandlululayo okanye ukuhlukumeza akuvumelekanga.";
"warn_general_item_2" = "Hlonipha Izithuba Zikawonke-wonke: Gcina iindlela zivulekile, thobela imithetho neempawu zasekuhlaleni, kwaye ungavaleli iindlela zetrafikhi okanye ezongxamiseko.";
"warn_general_item_3" = "Hlala Kwiindawo Ezimiselweyo: Ngenxa yokhuseleko lwakho, hlala ngaphakathi kwiindawo ezimiselwe isiganeko ukuba zikho.";
"warn_general_item_4" = "Yiba noxanduva: Ukuba ukhetha ukusela, yenza oko ngobunono kwaye uhambisane nemithetho yasekuhlaleni.";
"warn_general_item_5" = "Nciphisa Impembelelo Kwindalo: Lahla yonke inkunkuma kwizikhongozeli ezibekiwe uze ungahlwali ukuze iindawo zihlale zicocekile.";
"warn_general_item_6" = "Xela Iinkxalabo: Ukuba ubona into engatshongo kakuhle okanye umntu okwingxaki, xela kwangoko kugunyaziso lwasekuhlaleni.";
"warn_safety_title" = "Iingcebiso Zokhuseleko:";
"warn_safety_item_1" = "Nxiba Ngokufanelekileyo: Nxiba impahla nezihlangu ezitofotofo ezihambelana nemisebenzi yesiganeko kunye nemozulu.";
"warn_safety_item_2" = "Hlala Unamanzi: Zisa amanzi uze usele rhoqo, ingakumbi kwiziganeko ezifudumeleyo.";
"warn_safety_item_3" = "Qaphela Iimpahla Zakho: Gcina iimpahla zakho zikhuselekile uze uphephe ukuthwala izinto ezixabisekileyo ongazidingiyo.";
"warn_safety_item_4" = "Sebenzisa Inkqubo 'Yomhlobo': Hlala kunye nabahlobo okanye wenze abatsha, kwaye nijongane ukhuseleko lwenu omnye nomnye.";
"warn_safety_item_5" = "Lungiselela Iimeko Zongxamiseko: Gcina amanani ongxamiseko asekuhlaleni kwifowuni uze nivumelane nendawo yokuhlangana nabahlobo xa ningahlukana.";
"warn_emergency_title" = "Iinkqubo Zongxamiseko:";
"warn_emergency_item_1" = "Hlala Uqaphile: Chonga iindlela ezikhuselekileyo zokuphuma kunye neendawo ezivulekileyo (umz., izikwere, iipaki) ezikufutshane, uze ulandele imiyalelo yogunyaziso lwasekuhlaleni xa kusenzeka ingxamiseko.";
"warn_emergency_item_2" = "Amanani Ongxamiseko: Gcina inombolo yongxamiseko yendawo okuyo kunye neqhakamshelwano lakho lomntu ongxamiseko kwifowuni phambi kokuba ujoyine.";
"warn_emergency_item_3" = "Xa Kusenzeka Ingxamiseko: Biza inombolo yongxamiseko yasekuhlaleni, phuma ngokuzolileyo uye kwindawo evulekileyo ekhuselekileyo, nceda abanye ukuba uyakwazi, uze ugcine iindlela zokungena zivulekile.";
"warn_legal_title" = "Ezomthetho:";
"warn_legal_item_1" = "Ukukhululwa Kobutyala: Ukuthatha inxaxheba kwiziganeko ze-WorldWideWaves kukumngcipheko wakho. Ngokuthatha inxaxheba, uyavuma ukungabangi mabango kumenzi wesicelo ngokonzakala okunokwenzeka ngexesha lesiganeko.";
"warn_legal_item_2" = "Isaziso Sesithuba Sikawonke-wonke: Le yintlanganiso kawonke-wonke apho ukuthathwa kweefoto/ukurekhodwa kunokwenzeka ngababekhoyo okanye ziintatheli, ngokohambo lwemithetho yasekuhlaleni. Asibangi namalungelo kumfanekiso wakho kwaye asinakuwulawula umyalezo weqela lesithathu. Ukuba awufuni ukubonakala, zama ukuphepha iikhamera xa kunokwenzeka.";
"faq" = "FAQ";
"faq_access" = "Fikelela kwi-FAQ";
"faq_question_1" = "Kutheni kungekho msitho kwisixeko sam okanye kwilizwe lam?";
"faq_answer_1" = "Sikhethe amabini-khulu ezixeko ezinamaxesha amaninzi ehlabathini ukuze senze igagasi lehlabathi elinentsingiselo. Imida yezobugcisa neyokusebenza okwangoku ayisivumeli ukuxhasa iindawo ezingenamda. Sijolise kummeleli obanzi kwiilizwekazi, iinkcubeko neelwimi—into engenakugqwesa. Sinethemba lokongeza izixeko ezininzi ngokuhamba kwexesha.";
"faq_question_2" = "Zizintoni ezi ziflegi zamazwekazi?";
"faq_answer_2" = "Senze iiflegi zamazwekazi zokuqala, eziyimagqabhagqabha, ukunceda uchonge ngokukhawuleza izixeko kuluhlu nokubonisa umoya womanyano oxhaswa yi-WorldWideWaves. Ezi flegi azisesemthethweni kwaye zikho kuphela kule app; azijolise ukungcikiva okanye ukutshintsha naziphi na iisimboli zesizwe okanye zenkcubeko. Kuba akukho ziflegi ezisemthethweni zamazwekazi ehlabathi, senze ezethu ukuze kubekho ucaciso nenkxaxheba. Ukuba uyazithanda, zisebenzise ngokukhululekileyo.";
"faq_question_3" = "Ngubani ongasemva kwe-WorldWideWaves?";
"faq_answer_3" = "I-WorldWideWaves yinzame yabucala ngokupheleleyo. Akukho nkampani, mbutho, okanye urhulumente ongemva kwayo. Abadali bafuna ukunikezela indlela eyakhayo kuluntu oluzibuza indlela abanokuphucula ngayo ihlabathi neemeko zobomi—umyalezo ozama ukudibanisa zonke iinzame ezikhoyo ezihambele phambili nezentando yesininzi phantsi komqondiso wexolo nongathathi cala kwezopolitiko.";
"faq_question_4" = "Ingaba le yintshukumo yorhwebo?";
"faq_answer_4" = "Hayi. Usetyenziso lusimahla ngokupheleleyo. Nangona uyilo nophuhliso lwalufuna ixesha nemali kubadali, abangenelwa zimali buqu ngokuthe ngqo kulo.";
"faq_question_5" = "Ndinganegalelo kulo mzamo?";
"faq_answer_5" = "Ewe, abavolontiya bamkelekile rhoqo. Ikhowudi yesicelo ingumthombo ovulekileyo kwi GitHub, kwaye unokunxibelelana nabadali ku worldwidewaves@proton.me (iimpendulo azisiqinisekiswanga). Eyona ndlela ilungileyo yokunceda kukusasaza umyalezo: yabelana ngeefoto neevidiyo zemisitho usebenzisa ii-hashtag ezicetyisiweyo, qhagamshelana nabachaphazeli basekuhlaleni ukwandisa umyalezo, thetha ngalo noluntu lwakho, yaye okona kubaluleke kakhulu, joyina igagasi!";
"faq_question_6" = "Ndingalulingisa njani igagasi?";
"faq_answer_6" = "Cofa iqhosha elingezantsi ukuvula imo yolingiso. Kwizikrini zesiganeko, sebenzisa iqhosha lesibali-xesha elikufuphi neqhosha elithi \"Joyina igagasi\" ukuqalisa ulingiso usebenzisa iimpawu zesi siganeko (umhla, ixesha, indawo). Ukumisa ulingiso sebenzisa elo qhosha kwakhona. Okokugqibela, misa imo yolingiso usebenzisa i-icon yokumisa kumaleko ohlala ubonakala xa ugqibile.";
