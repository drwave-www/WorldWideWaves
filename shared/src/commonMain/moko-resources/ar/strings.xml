  <!-- src_hash: e9a4f3ee860dd68a4adb4f0e7a3e5c5181cba039 -->
  <string name="event_description_london_england">استمتع بأجواء Notting Hill Carnival متعددة الثقافات فيما ترقص هذه الموجة عبر الشوارع التاريخية، وحافلات double-decker الأيقونية، ونهر Thames الخالد.</string>
  <!-- src_hash: c2c832e4953ec104969891a521832e831335becb -->
  <string name="event_location_paris_france">باريس</string>
  <!-- src_hash: c190976c37f6ca1757d2903328be9dc2fdd26aaf -->
