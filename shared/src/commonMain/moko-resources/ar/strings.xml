  <!-- src_hash: 323da93d2967ec06961d6ead39228e669c6dad06 -->
  <string name="event_location_berlin_germany">برلين</string>
  <!-- src_hash: 07d2b86e5280ae406c1e8bcfcb2aa6068888c3fd -->
  <string name="event_description_berlin_germany">اشعر بروح الوحدة في German Unity Day بينما تعبر الموجة Brandenburg Gate وجدران الغرافيتي والعمارة الحديثة لهذه العاصمة النابضة.</string>
  <!-- src_hash: 7aa669c11145403c03e255dcc5561ba13289f1c4 -->
  <string name="event_location_madrid_spain">مدريد</string>
  <!-- src_hash: 75cde86a1f93104a93a9856d2d69284ea1b76bfc -->
