  <!-- src_hash: 15e68b4bf3c3f01ce266cd27fadd9d5cae9d86c2 -->
  <string name="event_description_buenos_aires_argentina">חגגו את עצמאות ארגנטינה לצלילי טנגו, בשדרות רחבות בסגנון אירופי, וברוחם התוססת של תושבי העיר לאורך ריו דה לה פלטה.</string>
  <!-- src_hash: e01153d790bec96b82a71d34fc7e4d062da5c2bb -->
  <string name="event_location_london_england">לונדון</string>
  <!-- src_hash: e9a4f3ee860dd68a4adb4f0e7a3e5c5181cba039 -->
  <string name="event_description_london_england">שמחו ברב-תרבותיות של קרנבל נוטינג היל כשהגל רוקד ברחובות ההיסטוריים, ליד אוטובוסים דו-קומתיים ולאורך התמזה הנצחי.</string>
  <!-- src_hash: c2c832e4953ec104969891a521832e831335becb -->
  <string name="event_location_paris_france">פריז</string>
  <!-- src_hash: c190976c37f6ca1757d2903328be9dc2fdd26aaf -->
  <string name="event_description_paris_france">ביום הבסטיליה הצטרפו לגל שנע ממגדל אייפל לשאנז-אליזה, וחוגגים את הרומנטיקה והיצירתיות של עיר האורות.</string>
  <!-- src_hash: 323da93d2967ec06961d6ead39228e669c6dad06 -->
  <string name="event_location_berlin_germany">ברלין</string>
